{
  "name": "preshape",
  "version": "0.0.0-development",
  "description": "A minimal design system and library of composable React components, primarily for my personal projects.",
  "repository": "git@github.com:HHogg/preshape.git",
  "author": "Harry Hogg <harry@hogg.io>",
  "license": "MIT",
  "type": "module",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "main": "dist/preshape.js",
  "module": "dist/preshape.js",
  "types": "dist/src/index.d.ts",
  "source": "./src",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "vite build && yarn build:docs && yarn prepare:packageJson",
    "build:docs": "typedoc src/index.ts --exclude lib --excludeExternals --json ../site/src/assets/documentation.json --jsDocCompatibility false",
    "prepare:packageJson": "node ./scripts/preparePackageJson.js",
    "tsc:check": "tsc --noEmit --skipLibCheck"
  },
  "peerDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^6.11.1"
  },
  "dependencies": {
    "@floating-ui/react": "0.26.24",
    "classnames": "^2.5.1",
    "framer-motion": "^11.2.11",
    "lodash.omit": "^4.5.0",
    "lucide-react": "^0.445.0",
    "luxon": "^3.3.0",
    "normalize.css": "^8.0.0",
    "open-color": "^1.6.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^6.11.1",
    "react-syntax-highlighter": "^15.5.0",
    "resize-observer-polyfill": "^1.5.0",
    "uuid": "10.0.0"
  },
  "devDependencies": {
    "@svgr/cli": "^8.1.0",
    "@types/lodash.omit": "^4.5.6",
    "@types/luxon": "^3.3.0",
    "@types/node": "^22.5.5",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/react-syntax-highlighter": "^15.5.6",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.0",
    "framer-motion": "^11.2.11",
    "postcss": "^8.3.11",
    "postcss-cli": "^11.0.0",
    "postcss-preset-env": "^10.0.3",
    "ts-node": "^10.9.1",
    "typedoc": "^0.26.7",
    "typescript": "^5.0.4",
    "vite": "^5.4.7",
    "vite-plugin-dts": "^4.2.0"
  }
