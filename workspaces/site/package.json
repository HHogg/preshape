{
  "name": "site",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "dev": "vite --host localhost --port 4001",
    "build": "yarn run clean && yarn run build:client && yarn run build:server && yarn run ssg",
    "build:client": "vite build --outDir dist/client",
    "build:svgs": "yarn build:svgs:pictograms && yarn build:svgs:logo",
    "build:svgs:logo": "yarn svgr ./src/assets/preshape-icon.svg --out-dir ./src/components/Logo --ext tsx --config-file .svgrrc.logo.cjs",
    "build:svgs:pictograms": "yarn svgr -d ./src/docs/catalog/pictograms ./src/assets/pictogram-svgs --ext tsx --config-file .svgrrc.pictograms.cjs",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "ssg": "node prerender"
  },
  "dependencies": {
    "lodash.omit": "^4.5.0",
    "luxon": "^3.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.1",
    "typedoc": "^0.24.7"
  },
  "devDependencies": {
    "@svgr/cli": "^5.5.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/lodash.omit": "^4.5.6",
    "@types/luxon": "^3.3.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "serve-static": "^1.15.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.2"
  }
}
