{
  "name": "preshape",
  "version": "0.0.0-development",
  "description": "A minimal design system and library of composable React components, primarily for my personal projects.",
  "main": "lib/index.js",
  "repository": "git@github.com:HHogg/preshape.git",
  "author": "Harry Hogg <harry@hogg.io>",
  "license": "MIT",
  "scripts": {
    "build:package": "rm -rf lib && mkdir lib && rsync -a --prune-empty-dirs --exclude '*.test.js' --exclude '__snapshots__/*' src/* lib && NODE_ENV=production BABEL_ENV=production  npx babel lib --out-dir lib",
    "build:site": "rm -rf public && parcel build site/index.html -d public && react-snap",
    "lint:css": "stylelint site/**/*.css src/**/*.css",
    "lint:js": "eslint site/**/*.js src/**/*.js",
    "lint": "yarn lint:js && yarn lint:css",
    "semantic-release": "semantic-release",
    "serve": "superstatic ./public --port 4000 -c superstatic.json",
    "start": "rm -rf dist && parcel site/index.html --port 4000 -d dist",
    "test": "BABEL_ENV=test jest ./src --config .jest.json",
    "travis-deploy-once": "travis-deploy-once"
  },
  "alias": {
    "preshape": "./src/index.js"
  },
  "peerDependencies": {
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash.debounce": "^4.0.8",
    "lodash.omit": "^4.5.0",
    "normalize.css": "^8.0.0",
    "open-color": "^1.6.3",
    "popper.js": "^1.14.4",
    "preshape": "^3.1.0",
    "prismjs": "^1.14.0",
    "react-popper": "^1.0.2",
    "react-transition-group": "^2.3.0",
    "remarkable": "^1.7.1",
    "remarkable-react": "^1.3.0",
    "resize-observer-polyfill": "^1.5.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^2.0.2",
    "@semantic-release/git": "^5.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "caniuse-lite": "^1.0.30000697",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-preshape": "^1.1.0",
    "eslint-plugin-react": "^7.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "lodash.pick": "^4.4.0",
    "parcel-bundler": "^1.12.3",
    "postcss-cssnext": "^3.1.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-snap": "^1.23.0",
    "react-test-renderer": "^16.3.2",
    "semantic-release": "^15.9.8",
    "stylelint": "^9.2.0",
    "stylelint-config-preshape": "^1.0.0",
    "stylelint-order": "^0.8.1",
    "superstatic": "^6.0.4",
    "travis-deploy-once": "^5.0.2"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.css$": "identity-obj-proxy"
    },
    "setupFiles": [
      "jest-setup.js"
    ]
  },
  "marked": null,
  "reactSnap": {
    "source": "public",
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": false
    }
  }
}
