{
  "name": "preshape",
  "version": "1.0.0",
  "description": "A minimal design system and library of composable React components, primarily for my personal projects.",
  "main": "lib/index.js",
  "repository": "https://github.com/HHogg/preshape",
  "author": "Harry Hogg <harry@hogg.io>",
  "license": "MIT",
  "scripts": {
    "build:package": "rm -rf lib && mkdir lib && rsync -a --prune-empty-dirs --exclude '*.test.js' --exclude '__snapshots__/*' src/* lib && NODE_ENV=production BABEL_ENV=production  npx babel lib --out-dir lib",
    "build:site": "NODE_ENV=production BABEL_ENV=static webpack --config webpack.static.config.js",
    "lint:css": "stylelint site/**/*.css src/**/*.css",
    "lint:js": "eslint site/**/*.js src/**/*.js",
    "lint": "yarn lint:js && yarn lint:css",
    "start": "NODE_ENV=development webpack-dev-server",
    "test": "BABEL_ENV=test jest ./src --config .jest.json"
  },
  "peerDependencies": {
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash.debounce": "^4.0.8",
    "lodash.omit": "^4.5.0",
    "normalize.css": "^8.0.0",
    "open-color": "^1.6.3",
    "prismjs": "^1.14.0",
    "react-transition-group": "^2.3.0",
    "remarkable": "^1.7.1",
    "remarkable-react": "^1.3.0",
    "resize-observer-polyfill": "^1.5.0"
  },
  "devDependencies": {
    "@brandwatch/axiom-documentation-loader": "^0.0.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "caniuse-lite": "^1.0.30000697",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "ejs-loader": "^0.3.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-preshape": "^1.0.0",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "lodash.pick": "^4.4.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.3",
    "raw-loader": "^0.5.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-hot-loader": "^4.0.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.3.2",
    "static-site-generator-webpack-plugin": "^3.4.1",
    "style-loader": "^0.20.3",
    "stylelint": "^9.2.0",
    "stylelint-config-preshape": "^1.0.0",
    "stylelint-order": "^0.8.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.0.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.css$": "identity-obj-proxy"
    },
    "setupFiles": [
      "jest-setup.js"
    ]
  }
}
